{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
            "@/*": ["./"],
            "@components/*": ["./components/*"],
            "@layouts/*": ["./layouts/*"],
            "@pages/*": ["./pages/*"],
            "@data/*": ["./data/*"]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "react"
    }
}
